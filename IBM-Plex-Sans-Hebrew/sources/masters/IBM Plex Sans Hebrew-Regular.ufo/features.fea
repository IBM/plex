		sub yodyod-hb patah-hb by yodyodpatah-hb;
		sub bet-hb rafe-hb by betrafe-hb;
		sub kaf-hb rafe-hb by kafrafe-hb;
		sub pe-hb rafe-hb by perafe-hb;
	} hebrewPresentationForms;
	lookup contextSubs {
		sub g' @acc_comb_top by g.alt02; # alternative g with lower ear
		sub i' @acc_comb_top by dotlessi;
		sub j' @acc_comb_top by dotlessj;
	} contextSubs;
} ccmp;



