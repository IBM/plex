feature ccmp {
	lookup thaiDecomposeSaraAm {
		sub saraAm-thai by nikhahit-thai saraAa-thai;
	} thaiDecomposeSaraAm;

	script thai;
	lookup thaiDecomposeSaraAm;
} ccmp;



feature calt {
	lookup thaiAltBases1 {
		# Use shortened loChula with marks above:
		sub loChula-thai' @thaiAboveMarks by loChula-thai.short;
	} thaiAltBases1;
	lookup thaiAltBases2 {
		# shorten bases to make room for marks below:
		lookupflag UseMarkFilteringSet @thaiBelowMarks;
		# we want to ignore any possible tildecomb on the base in Patani Malay, which would come between base and belowmarks;
		sub [doChada-thai toPatak-thai] ' @thaiBelowMarks by [doChada-thai.short toPatak-thai.short];
		# use .less versions with marks below:
		sub [yoYing-thai thoThan-thai] ' @thaiBelowMarks by [yoYing-thai.less thoThan-thai.less];
	} thaiAltBases2;
	lookup pataniMalay {
		# There's no language tag defined for this language:
		# Patani Malay uses macronbelow under certain consonants; these should be precomposed ligature glyphs:
		sub ngoNgu-thai macronbelowcomb by ngoNgu-thai_macronbelowcomb;
		sub [yoYing-thai yoYing-thai.less] macronbelowcomb by yoYing-thai_macronbelowcomb;
		sub noNu-thai macronbelowcomb by noNu-thai_macronbelowcomb;
		sub moMa-thai macronbelowcomb by moMa-thai_macronbelowcomb;
	} pataniMalay;
	lookup thaiNarrowMarks {
		# Use narrower marks [or same outlines but with amended anchors] on remaining bases with ascenders:
		# We need to ignore any below marks between the consonant and tone marks.
		lookupflag UseMarkFilteringSet @thaiAboveMarks;
		# *** Note the exact marks to be substituted will depend on the design ***
		sub [poPla-thai foFa-thai foFan-thai] [saraI-thai saraIi-thai saraUe-thai saraUee-thai maitaikhu-thai maiHanaKat-thai thanthakhat-thai maiChattawa-thai nikhahit-thai maiEk-thai maiTho-thai maiTri-thai yamakkan-thai] ' by [saraI-thai.narrow saraIi-thai.narrow saraUe-thai.narrow saraUee-thai.narrow maitaikhu-thai.narrow maiHanaKat-thai.narrow thanthakhat-thai.narrow maiChattawa-thai.narrow nikhahit-thai.narrow maiEk-thai.narrow maiTho-thai.narrow maiTri-thai.narrow yamakkan-thai.narrow];
	} thaiNarrowMarks;
	lookup thaiUpstairsMarkLigas {
		lookupflag 0;
		sub maiHanaKat-thai maitaikhu-thai by maiHanaKat_maitaikhu-thai;
		sub maiHanaKat-thai maiTri-thai by maiHanaKat_maiTri-thai;
	} thaiUpstairsMarkLigas;
	lookup thaiUpstairsMarks {
		# @thaiAboveMarks will cause a following tonemark to move up:
		# Reset lookup flags since nothing should occur between an abovemark and a tonemark.
		lookupflag 0;
		sub @thaiAboveMarks @toneMarks' by @toneMarksUp;
	} thaiUpstairsMarks;
	lookup nikhahit {
		# Take care of tonemarks above nikhahit:
		sub @toneMarks' [nikhahit-thai nikhahit-thai.narrow] by @toneMarksUp;
		#sub [nikhahit-thai nikhahit-thai.narrow] @toneMarks' by @toneMarksUp;
	} nikhahit;

	script thai;
	lookup thaiAltBases1;
	lookup thaiAltBases2;
	lookup pataniMalay;
	lookup thaiNarrowMarks;
	lookup thaiUpstairsMarkLigas;
	lookup thaiUpstairsMarks;
	lookup nikhahit;

	script thai;
	language PAL;
	lookup thaiAltBases1;
	lookup thaiNarrowMarks;
	lookup thaiUpstairsMarkLigas;
	lookup thaiUpstairsMarks;
	lookup nikhahit;
	# Use .less versions of yoYing and thoThan as defaults:
	sub [yoYing-thai thoThan-thai] by [yoYing-thai.less thoThan-thai.less];
	sub [doChada-thai toPatak-thai] ' @thaiBelowMarks by [doChada-thai.short toPatak-thai.short];

	script thai;
	language SAN;
	lookup thaiAltBases1;
	lookup thaiNarrowMarks;
	lookup thaiUpstairsMarkLigas;
	lookup thaiUpstairsMarks;
	lookup nikhahit;
	# Use .less versions of yoYing and thoThan as defaults:
	sub [yoYing-thai thoThan-thai] by [yoYing-thai.less thoThan-thai.less];
	sub [doChada-thai toPatak-thai] ' @thaiBelowMarks by [doChada-thai.short toPatak-thai.short];
} calt;



feature ordn {
	# ordinals
	sub [a o] by [ordfeminine ordmasculine];
} ordn;



feature frac {
	# fractions
	sub @numbers_dflt by @numbers_sups;
	sub [slash] by fraction;
	sub fraction @numbers_sups' by @numbers_sinf;
	sub @numbers_sinf @numbers_sups' by @numbers_sinf;
} frac;



feature numr {
	# numerators
	sub @numbers_dflt by @numbers_sups;
} numr;



feature dnom {
	# denominators
	sub @numbers_dflt by @numbers_sinf;
} dnom;



feature sups {
	# superiors
	lookup latinSuperialNumbers {
		sub @numbers_dflt by @numbers_sups;
	} latinSuperialNumbers;
	lookup thaiSuperialNumbers {
		script thai;
		sub zero-thai by zero-thai.sups;
		sub one-thai by one-thai.sups;
		sub two-thai by two-thai.sups;
		sub three-thai by three-thai.sups;
		sub four-thai by four-thai.sups;
		sub five-thai by five-thai.sups;
		sub six-thai by six-thai.sups;
		sub seven-thai by seven-thai.sups;
		sub eight-thai by eight-thai.sups;
		sub nine-thai by nine-thai.sups;
	} thaiSuperialNumbers;
} sups;



feature sinf {
	# inferiors
	sub @numbers_dflt by @numbers_sinf;
} sinf;



feature zero {
	# slashed zero
	sub zero by zero.alt02;
} zero;



feature ss01 {
	featureNames {
		name 3 1 0x0409 "simple lowercase a"; # Win / Unicode / English US
		name 1 0 0 "simple lowercase a"; #   Mac / Roman / English
	};
	# stylistic set 1 - single storey a
	sub @lca_dflt by @lca_alt1;
} ss01;



feature ss02 {
	featureNames {
		name 3 1 0x0409 "simple lowercase g"; # Win / Unicode / English US
		name 1 0 0 "simple lowercase g"; #   Mac / Roman / English
	};
	# stylistic set 2 - single storey g
	sub @lcg_dflt by @lcg_alt1;
} ss02;



feature ss03 {
	featureNames {
		name 3 1 0x0409 "slashed number zero"; # Win / Unicode / English US
		name 1 0 0 "slashed number zero"; #   Mac / Roman / English
	};
	# slashed zero
	sub zero by zero.alt02;
} ss03;



feature ss04 {
	featureNames {
		name 3 1 0x0409 "dotted number zero"; # Win / Unicode / English US
		name 1 0 0 "dotted number zero"; #   Mac / Roman / English
	};
	# dotted zero
	sub zero by zero.alt01;
} ss04;



feature ss05 {
	featureNames {
		name 3 1 0x0409 "alternate lowercase eszett"; # Win / Unicode / English US
		name 1 0 0 "alternate lowercase eszett"; #   Mac / Roman / English
	};
	# alternative german eszett
	sub germandbls by germandbls.alt01;
} ss05;



feature salt {
	# stylistic alternates - all stylistic sets combined
	sub @lca_dflt by @lca_alt1;
	sub @lcg_dflt by @lcg_alt1;
	sub zero by zero.alt01;
	sub germandbls by germandbls.alt01;
} salt;



feature liga {
	lookup latinLigatures {
		sub f i by fi;
	} latinLigatures;
	lookup thaiLigatures {
		script thai;
		sub lu-thai lakkhangyao-thai by lu_lakkhangyao-thai;
		sub ru-thai lakkhangyao-thai by ru_lakkhangyao-thai;
	} thaiLigatures;
} liga;



