		pos [nikhahit-thai nikhahit-thai.narrow] maiTri-thai.small' <145 -20 0 0>;
	} mkmkAdjustments;

	lookupflag 0;
	script thai;
	lookup mkmk_thai_bottomright;
	lookup mkmk_thai_topright;
	lookup mkmkAdjustments;
	lookupflag 0;

} mkmk;


